
{
  "project": "Ora Admin Web Interface",
  "spec": "./ORA_ADMIN_WEB_INTERFACE_SPEC.md",
  "agents_dir": "./agents",
  "order": [
    ["firebase-agent"],
    ["auth-agent", "rbac-agent"],
    ["api-agent"],
    ["storage-agent"],
    ["ui-agent", "commands-agent"],
    ["stats-agent"],
    ["docs-agent"]
  ],
  "gates": {
    "auth": ["tests/e2e/auth.spec.ts", "tests/unit/rbac.spec.ts"],
    "api": ["tests/unit/api/*.spec.ts"],
    "docs": ["README.md", ".env.example"]
  },
  "env_required": [
    "FIREBASE_SERVICE_ACCOUNT_JSON",
    "NEXT_PUBLIC_FIREBASE_PROJECT_ID",
    "NEXT_PUBLIC_FIREBASE_API_KEY",
    "NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN",
    "NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET",
    "NEXT_PUBLIC_FIREBASE_APP_ID"
  ]
}
